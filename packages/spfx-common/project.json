{
  "name": "spfx-common",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/spfx-common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/spfx-common",
        "main": "packages/spfx-common/src/index.js",
        "tsConfig": "packages/spfx-common/tsconfig.lib.json",
        "assets": ["packages/spfx-common/templates/**"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/spfx-common/jest.config.js"
      }
    }
  },
  "tags": []
}
